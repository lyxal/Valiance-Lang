#define #stack dip[T]: {(func: ð”½, top: T) =>
  `f` $top
}

#define #stack both: {(func: ð”½) =>
  `@f` ~> temp
  `f` $temp detuple
}

#define #stack fork: {(first: ð”½, second: ð”½) =>
  @($first) #peek ~> firstRes
  `@second` ~> secondRes
  $firstRes $secondRes both: detuple
}

#define #stack correspond: {(first: ð”½, second: ð”½) =>
  `@first` ~> temp
  `@second` $temp \ both: detuple
}
