define find(xs: T+, x: T) -> Number? {
    $ind = 0
    $remaining = $xs
    $found = false
    while ($remaining nonempty? $found not and) {
        if ($remaining[0] $x ===) {
            $found = true
        } else {
            $ind: ++
            $remaining: firstless
        }
    }
    [None, $ind] $[$found]
}

